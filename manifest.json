{
  "manifest_version": 2,
  "name": "Web Page Notes",
  "version": "0.0.0.3",
  "description": "This extension is a easy way to add a note to a page.",
  "icons": {
    "16": "icons.iconarchive.com/icons/guillendesign/variations-1/16/Notes-2-icon.png",
    "24": "icons.iconarchive.com/icons/guillendesign/variations-1/24/Notes-2-icon.png",
    "32": "icons.iconarchive.com/icons/guillendesign/variations-1/32/Notes-2-icon.png",
    "48": "icons.iconarchive.com/icons/guillendesign/variations-1/48/Notes-2-icon.png",
    "64": "icons.iconarchive.com/icons/guillendesign/variations-1/64/Notes-2-icon.png",
    "72": "icons.iconarchive.com/icons/guillendesign/variations-1/72/Notes-2-icon.png",
    "96": "icons.iconarchive.com/icons/guillendesign/variations-1/96/Notes-2-icon.png",
    "128": "icons.iconarchive.com/icons/guillendesign/variations-1/128/Notes-2-icon.png",
    "256": "icons.iconarchive.com/icons/guillendesign/variations-1/256/Notes-2-icon.png"
  },
  "author": "Abbas Gussenov",
  "page_action": {
    "default_icon": {
      "16": "icons.iconarchive.com/icons/fatcow/farm-fresh/16/note-add-icon.png",
      "24": "icons.iconarchive.com/icons/fatcow/farm-fresh/24/note-add-icon.png",
      "32": "icons.iconarchive.com/icons/fatcow/farm-fresh/32/note-add-icon.png"
    },
    "default_popup": "note.html",
    "default_title": "Add a Note"
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "short_name": "WebPageNotes",
  "background": {
    "page": "background.html"
  }
}
